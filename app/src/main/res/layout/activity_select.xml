<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".structure.body.management.activities.SelectActivity">

    <Button
        android:id="@+id/singleBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="single"
        android:text="Single"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="127dp" />

    <Button
        android:id="@+id/familyBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="family"
        android:text="family"
        tools:layout_editor_absoluteX="157dp"
        tools:layout_editor_absoluteY="269dp" />

    <Button
        android:id="@+id/logOutFromSelectBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="logOutFromSelect"
        android:text="log out"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="64dp" />

</androidx.constraintlayout.widget.ConstraintLayout>